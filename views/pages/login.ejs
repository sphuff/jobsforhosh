<%- contentFor('links') %>
<link rel="stylesheet" href="css/login.css">

<%- contentFor('main') %>
<div class="main-wrapper">
    <div class="header">
        <img src="http://images.nationalgeographic.com/wpf/media-live/photos/000/090/cache/african-elephant-standing_9033_600x450.jpg" />
    </div>
    <div class="content-wrapper">
        <div class="form-wrapper">
            <form>
                <input type="text" placeholder="Email">
            </form>
            <form>
                <input type="text" placeholder="Password">
            </form>
        </div>
        <div class="button-wrapper">
            <!-- <button class="primary-button" onclick="getBrowser()">LOG IN</button> -->
            <form action="/login" method="post">
              <button class="primary-button" onclick="getBrowser()">LOG IN</button>
            </form>
            <form action="/signup">
              <button class="secondary-button" onclick="getBrowser()">SIGN UP</button>
            </form>
            <!-- <button class="secondary-button" onclick="getBrowser()">SIGN UP</button> -->
        </div>
    </div>
</div>